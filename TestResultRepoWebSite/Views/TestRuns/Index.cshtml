
@{
    ViewBag.Title = "Index";
}

<div class="container">
    <div class="row">
        <div class="col-sm-6 text-center">
            <h3>@ViewBag.testRun.Name</h3>
        </div>
        <div class="col-sm-6 text-center">
            <h3>@ViewBag.testRun.StartTime</h3>
        </div>
    </div>
</div>
<div class="container">
    <div class="row">
        <div class="col-sm-12 text-center">
            <h1>Result</h1>
        </div>
    </div>
</div>
<div class="progress" style="height: 100px;">
    <div class="progress-bar bg-success" role="progressbar" style="width: @ViewBag.passedPercent%" aria-valuenow=@ViewBag.passedPercent aria-valuemin="0" aria-valuemax="100"></div>
    <div class="progress-bar bg-danger" role="progressbar" style="width: @ViewBag.failedPercent%" aria-valuenow=@ViewBag.failedPercent aria-valuemin="0" aria-valuemax="100"></div>
    <div class="progress-bar bg-warning" role="progressbar" style="width: @ViewBag.inconclusivePercent%" aria-valuenow=@ViewBag.inconclusivePercent aria-valuemin="0" aria-valuemax="100"></div>
</div>